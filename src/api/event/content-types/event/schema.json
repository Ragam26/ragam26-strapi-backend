{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date"
    },
    "eventName": {
      "type": "string",
      "required": true
    },
    "regFee": {
      "type": "integer"
    },
    "expDate": {
      "type": "date"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "makeMyPassUrl": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Flagship Events ",
        "Dramatics ",
        "Kalolsavam (group)",
        "Kalolsavam -solo-pass",
        "M&D-pass",
        "General-Pass",
        "Other"
      ]
    }
  }
}
